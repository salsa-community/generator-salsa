{
  "<%= page.name.dashCase%>": {
    "title": "<%= page.title%>",
    "subtitle": "<%= page.title%>",
    <%_ let campos = Object.keys(page.properties); _%>
    <%_ campos.forEach((key, idx) => { _%>
    <%_ let campo = page.properties[key]; _%>
    "<%= campo.name.dashCase%>": {
      "label": "<%= campo.title%>",
      "description": "<%= campo.description%>",
      "tooltip": "<%= campo.tooltip%>",
      "placeHolder": "<%= campo.placeHolder%>",
      "validations": {
        "required": "El campo <%= campo.title%> es requerido",
        "minMessage": "El número de caracteres debe ser de al menos {min}",
        "maxMessage": "El número de caracteres debe ser máximo de {max}",
        "regexMessage": "Formato de <%= campo.title%> inválido"
      }<%_ if (campo.type === 'array') { _%>,
        <%_ let items = Object.keys(campo.items.properties); _%>
        <%_ items.forEach((key, idx) => { _%>
          <%_ let item = campo.items.properties[key]; _%>
      "<%= item.name.dashCase%>": {
        "label": "<%= item.title%>",
        "description": "<%= item.description%>",
        "tooltip": "<%= item.tooltip%>",
        "placeHolder": "<%= item.placeHolder%>",
        "validations": {
          "required": "El campo <%= item.title%> es requerido",
          "minMessage": "El número de caracteres debe ser de al menos {min}",
          "maxMessage": "El número de caracteres debe ser máximo de {max}",
          "regexMessage": "Formato de <%= item.title%> inválido"
        }
      }<% if (items.length -1 !== idx ) { %>,<% } %>
        <%_ }) _%>
      <%_ } _%>
    }<% if (campos.length -1 !== idx ) { %>,<% } %>
    <%_ }) _%>
  }
}
